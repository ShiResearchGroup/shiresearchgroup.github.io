title = {{Current Advances in Characterization of Nano-porous Materials: Pore Size Distribution and Surface Area}},
url = {https://doi.org/10.1007/978-3-030-65991-2_12 http://link.springer.com/10.1007/978-3-030-65991-2_12},
year = {2021}
}
