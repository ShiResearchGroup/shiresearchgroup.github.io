issn = {1543-8384},
journal = {Molecular Pharmaceutics},
keywords = {gene carrier,polycation,polymer,simulation,triblock},
month = {aug},
number = {8},
pages = {2834--2844},
pmid = {26076229},
title = {{Coarse-Grained Simulation of Polycation/DNA-Like Complexes: Role of Neutral Block}},
url = {http://pubs.acs.org/doi/abs/10.1021/mp500861c http://pubs.acs.org/doi/10.1021/mp500861c},
